<Window x:Name="WindowStartupLocation" x:Class="WpfCookies.list"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="白名单" Height="450" Width="300" Icon=" resources/cookie1.png" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <StackPanel Orientation="Vertical">
            
            <GroupBox Height=" 350" Header=" 白名单">
                <StackPanel Orientation="Vertical">
                    <TextBlock Height="20">例子：qq.com或qq   一行输入一个关键字</TextBlock>
                    <TextBox x:Name="tb" TextWrapping="Wrap" AcceptsReturn="True" VerticalScrollBarVisibility="Visible" Height=" 300"></TextBox> 
                </StackPanel>
            </GroupBox>
            <Button Height=" 50" Width=" 50" Click="Button_Click">
                <Image Source="resources/Save.png"></Image>
            </Button>
        </StackPanel>
        
        
    </Grid>
</Window>
